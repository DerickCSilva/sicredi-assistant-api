{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "class-methods-use-this": "off",
        "array-callback-return": "warn",
        "no-console": "off",
        "default-case": "off",
        "no-case-declarations": "off",
        "no-extend-native": "off",
        "no-param-reassign": "off"
    }
}

